{
  "name": "@seognil-lab/la-starter-cli",
  "version": "0.1.1",
  "description": "My Javascript development cli",
  "repository": "https://github.com/seognil-lab/la-starter-cli",
  "author": "Seognil LC",
  "license": "MIT",
  "bin": {
    "lcli": "./src/index.js"
  },
  "scripts": {
    "format": "eslint --fix . && prettier --write '{,!(node_modules|dist)/**/}*.*'",
    "format:staged": "lint-staged && pretty-quick --staged"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:staged",
      "pre-push": "npm run format:staged"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "dependencies": {
    "@darkobits/lolcatjs": "^3.1.3",
    "app-root-path": "^2.2.1",
    "chalk": "^2.4.2",
    "command-exists": "^1.2.8",
    "commander": "^2.20.0",
    "download-git-repo": "^2.0.0",
    "execa": "^2.0.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.4.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jsonfile": "^5.0.0",
    "license-generator": "^0.0.13",
    "nodegit": "^0.24.3",
    "npm-conf": "^1.1.3",
    "ora": "^3.4.0",
    "shelljs": "^0.8.3",
    "trash": "^6.0.0"
  }
}
